<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shubzz.securitycheck_version1.Fragment_Home">

    <ImageView
        android:id="@+id/imageButton2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="53dp"
        android:layout_marginTop="65dp"
        app:srcCompat="@drawable/one_box" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_marginEnd="60dp"
        app:srcCompat="@drawable/one_box" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignStart="@+id/imageButton2"
        android:layout_below="@+id/imageButton2"
        android:layout_marginTop="80dp"
        app:srcCompat="@drawable/one_box" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignStart="@+id/imageView3"
        android:layout_alignTop="@+id/imageView4"
        app:srcCompat="@drawable/one_box" />

    <TextView
        android:id="@+id/Supervisor_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageButton2"
        android:layout_below="@+id/imageButton2"
        android:layout_marginEnd="12dp"
        android:layout_marginTop="13dp"
        android:text="SUPERVISOR"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/Guards_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageView3"
        android:layout_alignTop="@+id/Supervisor_TextView"
        android:layout_marginEnd="10dp"
        android:text="GUARDS"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/Areas_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/attnIntent"
        android:layout_below="@+id/imageView4"
        android:text="AREAS"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/Reports_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/Guards_TextView"
        android:layout_alignTop="@+id/Areas_TextView"
        android:text="REPORT"
        android:textColor="#0a0a0a" />

    <ImageView
        android:id="@+id/attnIntent"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/imageView4"
        android:layout_alignEnd="@+id/Supervisor_TextView"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="13dp"
        app:srcCompat="@drawable/group" />

    <ImageView
        android:id="@+id/sugg_intent"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/imageView9"
        android:layout_alignTop="@+id/attnIntent"
        app:srcCompat="@drawable/group" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/Areas_TextView"
        android:layout_marginBottom="188dp"
        app:srcCompat="@drawable/group" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageView8"
        android:layout_marginEnd="83dp"
        app:srcCompat="@drawable/group" />


</RelativeLayout>
